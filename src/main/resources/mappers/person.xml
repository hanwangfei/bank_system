<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.rog//DTD Mapper 3.0//EN"
        "http://mybatis.org//dta/mybatis-3-mapper.dtd">
<mapper namespace="com.hwf.oa.dao.PersonDao">

    <select id="selectByIdentityId" parameterType="Long" resultType="com.hwf.oa.entity.Person">
        select * from person where identity_id=#{value}
    </select>

</mapper>